{
  "name": "streamss-cat",
  "description": "Concatenate stream2 streams to one stream",
  "version": "0.1.0",
  "main": "index.js",
  "engines": {
    "node": ">=0.8.0"
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "dependencies": {
    "streamss": "~0.2.5",
    "streamss-readonly": "~0.1.0",
    "streamss-shim": "~0.0.2"
  },
  "devDependencies": {
    "mocha": "latest"
  },
  "scripts": {
    "test": "mocha test/*.mocha.js",
    "cover": "istanbul cover _mocha --report lcov --report text -- -R dot --check-leaks test/*.mocha.js",
    "lint": "eslint '*.js' '*/**.js'",
    "doc": "jsdoc -d doc -c jsdoc.json index.js lib/*.js",
    "clean": "rm -rf doc coverage"
  },
  "repository": {
    "type": "git",
    "url": "github.com/commenthol/streamss-cat"
  },
  "keywords": [
    "stream2",
    "concat"
  ],
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": "commenthol <commenthol@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commenthol/streamss-cat/issues"
  },
  "homepage": "https://github.com/commenthol/streamss-cat"
}
